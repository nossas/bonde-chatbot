{
  "name": "chatbot",
  "version": "3.0.0",
  "description": "Enable activist to pressure through facebook",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev-old": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "dev": "tsc-watch --onSuccess \"node ./dist/playground/index.js\"",
    "start": "node ./dist/playground/index.js",
    "start:worker": "node ./dist/workers/process-bot-mass-message-queue.js",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint . --ext js,ts,tsx",
    "docs": "documentation readme src/**/*.ts --section=install",
    "postdocs": "git add README.md",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --ext js,ts,tsx",
      "git add"
    ]
  },
  "keywords": [
    "generator-nod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nossas/bonde-bot.git"
  },
  "author": "Lucas Pirola<pirola@nossas.org>",
  "license": "GPL-3.0",
  "dependencies": {
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link": "1.2.13",
    "apollo-utilities": "1.3.2",
    "apollo-link-http": "1.5.15",
    "apollo-link-ws": "1.0.18",
    "subscriptions-transport-ws": "0.9.16",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bull": "^3.12.1",
    "colors": "^1.4.0",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "isomorphic-fetch": "2.2.1",
    "jwt-decode": "^2.2.0",
    "messenger-bot": "nossas/messenger-bot#master",
    "morgan": "^1.9.1",
    "node-fetch": "2.6.0",
    "node-wit": "^4.3.0",
    "pug": "^2.0.4",
    "querystring": "0.2.0",
    "underscore": "^1.9.1",
    "dotenv": "^8.2.0",
    "ws": "7.1.2"
  },
  "devDependencies": {
    "@types/bull": "3.10.3",
    "@types/cors": "2.8.6",
    "@types/dateformat": "3.0.1",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/express-session": "1.15.14",
    "@types/jest": "24.0.18",
    "@types/jwt-decode": "2.2.1",
    "@types/morgan": "1.7.37",
    "@types/node": "12.7.8",
    "@types/node-wit": "4.2.2",
    "@types/underscore": "1.9.3",
    "documentation": "12.1.1",
    "husky": "3.0.2",
    "lint-staged": "9.2.1",
    "open-cli": "5.0.0",
    "rimraf": "2.6.3",
    "standard-version": "7.0.0"
  }
}
